  "author": "Tobias Bildner",
  "license": "BSD",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^5.1.1",
    "@types/node-fetch": "^2.6.6",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "@typescript-eslint/parser": "^6.7.4",
    "@vercel/ncc": "^0.38.0",
    "eslint": "^8.51.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.4.2",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
